declare
    function count
        if ten > 5
            sub ten 1
            read ten
            call count
            return
            dialog "Not reachable num"
        endif

        if zero < 5
            add zero 1
            read zero
            call count
            return
            dialog "Not reachable zero"
        endif
        
        dialog "Call this only one time :D"
    endfunction

    function otherDialog
        dialog "Test test test"
        read test
    endfunction

    function calculus
        mul num 3
        read num

        div num 2
        read num
    endfunction

    variable ten 10
    variable zero 0
    variable hello "Hello"
    variable world "World!"
    variable test1 "test1: $hello$"
    variable test2 "test2: $test1$"
enddeclare

call count
read test2