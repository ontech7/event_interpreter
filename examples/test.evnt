declare
    function count
        if ten > 5
            sub ten 1
            read ten
            call count
            return
            dialog "Not reachable num"
        endif

        if zero < 5
            add zero 1
            read zero
            call count
            return
            dialog "Not reachable zero"
        endif
        
        dialog "Call this only one time :D"
    endfunction

    function otherDialog
        dialog "Test test test"
        read test
    endfunction

    function between
        if ten < 10
            if ten > 0
                if zero = 0
                    sub ten 1
                    read ten
                endif
            endif
        endif
    endfunction

    function ifelse
        if ten
            dialog "ten"
            if zero
                dialog "zero"
                if hello
                    dialog $hello$
                endif
                dialog "not zero"
            endif
            dialog "not ten"
        endif
        dialog "out"
    endfunction

    variable ten 10
    variable zero 0
    variable hello "Hello"
    variable world "World!"
    variable test1 "test1: $hello$"
    variable test2 "test2: $test1$"
    variable test3 "jkjk $const1$"
    constant CONST1 2
enddeclare

call ifelse