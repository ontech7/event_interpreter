declare
    function countdown
        if num > 5
            sub num 1
            call countdown
        endif
    endfunction

    function otherDialog
        dialog "Test test test"
        read test
    endfunction

    variable num 10
    variable hello "Hello"
    variable world "World!"
    variable test "Let's read this var"
enddeclare

#call countdown

#read num

dialog "$hello$ $world$"

set num 2

dialog $test$